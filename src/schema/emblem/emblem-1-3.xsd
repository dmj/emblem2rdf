		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="physical"/>
			<xs:enumeration value="logical"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="medPictura">
		<xs:annotation>
